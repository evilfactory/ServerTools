<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('templates/header.ejs') %>

        <title>Barotrauma Server Manager</title>

</head>

<body>
    <%- include('templates/navbar.ejs') %>

        <div class="container">

            <% Object.entries(baroMS.config.barotraumaServers).forEach(entry=> { const [key, value] = entry %>
                <h1>
                    <a href="/servers/<%= key %>/home">
                        <%= value.Name %>
                    </a>
                </h2>
            <% }); %>
        </div>
</body>

</html>